  "private": true,
  "scripts": {
    "build": "webpack",
    "hack": "webpack && webpack-dev-server --inline",
    "postinstall": "webpack && yarn migrate && yarn seed",
    "start": "node server/bin/www",
    "test": "jest --forceExit",
    "test:watch": "yarn test -- --watch",
    "test:login": "mocha --reporter nyan ./server/tests/authentication.js --exit",
    "migrate": "knex migrate:latest --knexfile server/db/knexfile.js",
    "rollback": "knex migrate:rollback --knexfile server/db/knexfile.js",
    "seed": "knex seed:run --knexfile server/db/knexfile.js",
    "resetdb": "yarn rollback && yarn migrate && yarn seed"
